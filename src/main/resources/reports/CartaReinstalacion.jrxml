			<textField isBlankWhenNull="true">
				<reportElement x="0" y="317" width="435" height="23" uuid="d93c8608-e8b2-4f2b-b918-3bb24849be3e"/>
				<box leftPadding="6" rightPadding="6">
					<pen lineWidth="0.25" lineColor="#CCCCCC"/>
